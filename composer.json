{
    "name": "lonnylot/telnyx-php-sdk",
    "description": "Telnex PHP SDK",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "ramsey/uuid": "^3.9"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Lonny Kapelushnik",
            "email": "lonny@lonnylot.com"
        }
    ],
    "require": {
        "lonnylot/crud-sugar": "^1.0"
    },
    "autoload": {
      "psr-4": {
        "Lonnylot\\Telnyx\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Lonnylot\\Telnyx\\Laravel\\TelnyxServiceProvider"
        ]
      }
    }
}
